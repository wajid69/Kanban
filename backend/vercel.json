{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@DATABASE_URL",
    "ORIGIN": "@ORIGIN",
    "PORT": "5000",
    "NODE_ENV": "production"
  }
}
